GLOB_MKDIRS := $(sort $(GLOB_MKDIRS))
ifneq ($(GLOB_MKDIRS),)
$(GLOB_MKDIRS) :
	@$(MKDIR) $@
endif

include $(SRCDIR)/tools/genmodule/Makefile.deps

# Rules for the AROS build tools
$(GENMODULE) : $(GENMODULE_DEPS)
	@$(CALL) $(MAKE) $(MKARGS) -C $(SRCDIR)/tools/genmodule TOP=$(TOP) SRCDIR=$(SRCDIR) $(GENMODULE)

